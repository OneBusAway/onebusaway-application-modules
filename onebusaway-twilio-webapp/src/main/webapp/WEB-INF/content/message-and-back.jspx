<Response xmlns:jsp="http://java.sun.com/JSP/Page"
      xmlns:c="http://java.sun.com/jsp/jstl/core"
     xmlns:s="/struts-tags"
     xmlns:wiki="/oba-wiki-tags">
  <jsp:directive.page contentType="application/xml" />
  <Gather numDigits="1">
    <Say>
      ${message}
    </Say>
    <Say>
      If you wish to return to the previous menu, please press star.
    </Say>
  </Gather>
</Response>
