	<s:url var="url" value="/js/lib/jquery.js" />
	<script type="text/javascript" src="${url}">
	<!-- //prevent jspx minimization -->
	</script>
	
	<s:url var="url" value="/js/lib/jquery.ui.js" />
	<script type="text/javascript" src="${url}">
	<!-- //prevent jspx minimization -->
	</script>
	
	<s:url var="url" value="/js/lib/grid.locale-en.js" />
	<script type="text/javascript" src="${url}">
	<!-- //prevent jspx minimization -->
	</script>
	
	<s:url var="url" value="/js/lib/jqGrid.js" />
	<script type="text/javascript" src="${url}">
	<!-- //prevent jspx minimization -->
	</script>
	
	
	<s:url var="url" value="/js/lib/ember.js" />
	<script type="text/javascript" src="${url}">
	<!-- //prevent jspx minimization -->
	</script>
	
